{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Items</h1>
    <ul>
        {% for item in items %}
            <li><a href="/Admin/show/{{ item.id }}">{{ item.id }}. {{ item.name }}</a></li>
        {% else %}
            <li>Nothing to display</li>
        {% endfor %}
    </ul>
    <a href="/Admin/add">Ajouter</a>
    <a href="/target/index">Retour</a>
{% endblock %}
